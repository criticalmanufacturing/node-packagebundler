    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Start PackagePacker",
            "program": "${workspaceRoot}/node_modules/yo/lib/cli.js",
            "args": [
                "${workspaceRoot}/generators/packagePacker/index.js",
                "-a=C:/cmf/repos/Product/IoMT/dependencies/IoTNodeAddons",
                "-d",
                "-o=."
            ],
            "cwd": "C:/cmf/repos/MonoRepo/Product/Platform/ConnectIoT/src/connect-iot-business-scenarios-general",
            "outFiles": [],
            "outputCapture": "std"
        },
        {
            "name": "Run tests on Package Packer",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "stopOnEntry": false,
            "args": [
                "test/**/*.test.js",
                "test/*.test.js",
                "--no-timeouts"
            ],
            "cwd": "${workspaceRoot}/generators/packagePacker",
            "runtimeExecutable": null,
            "sourceMaps": true,
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Create Driver",
            "program": "${workspaceRoot}/node_modules/yo/lib/cli.js",
            "args": [
                "${workspaceRoot}/generators/driver/index.js"
            ],
            "cwd": "C:/Temp",
            "outFiles": [],
            "outputCapture": "std",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Create Task Library",
            "program": "${workspaceRoot}/node_modules/yo/lib/cli.js",
            "args": [
                "${workspaceRoot}/generators/tasksLibrary/index.js"
            ],
            "cwd": "C:/cmf/repos/DS/BBraunBaseline/Features/Cmf.Custom.Baseline/Cmf.Custom.Baseline.IoT/Cmf.Custom.Baseline.IoT.Packages/src/",
            "outFiles": [],
            "outputCapture": "std",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Create Task",
            "program": "${workspaceRoot}/node_modules/yo/lib/cli.js",
            "args": [
                "${workspaceRoot}/generators/task/index.js"
            ],
            "cwd": "C:/cmf/repos/DS/BBraunBaseline/Features/Cmf.Custom.Baseline/Cmf.Custom.Baseline.IoT/Cmf.Custom.Baseline.IoT.Packages/src/controller-engine-custom-tasks",
            "outFiles": [],
            "outputCapture": "std",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Create Converter",
            "program": "${workspaceRoot}/node_modules/yo/lib/cli.js",
            "args": [
                "${workspaceRoot}/generators/converter/index.js"
            ],
            "cwd": "C:/cmf/repos/DS/BBraunBaseline/Features/Cmf.Custom.Baseline/Cmf.Custom.Baseline.IoT/Cmf.Custom.Baseline.IoT.Packages/src/controller-engine-custom-tasks",
            "outFiles": [],
            "outputCapture": "std",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
        }
    ]
}